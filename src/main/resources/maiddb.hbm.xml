<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "https://hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.example.server.model.ListElement" table="ListElements">
        <id name="id" type="int">
            <column name="ID" />
            <generator class="increment" />
        </id>
        <property name="list" type="java.lang.String">
            <column name="LIST" />
        </property>
        <property name="element" type="java.lang.String">
            <column name="ELEMENT" />
        </property>
    </class>
</hibernate-mapping>